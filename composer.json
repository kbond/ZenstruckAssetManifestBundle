{
    "name": "zenstruck/asset-manifest-bundle",
    "description": "Loads an asset manifest to map your assets to dynamic ones.",
    "keywords": ["asset", "frontent", "gulp", "grunt", "css", "javascript", "twig"],
    "homepage": "http://zenstruck.com/project/ZenstruckAssetManifestBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/twig-bundle": "^2.8|^3.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^2.8|^3.0",
        "symfony/css-selector": "^2.8|^3.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\AssetManifestBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\AssetManifestBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
